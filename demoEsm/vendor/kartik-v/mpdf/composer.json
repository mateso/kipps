    "keywords": ["php", "pdf", "utf-8"],
    "homepage": "http://www.mpdf1.com/mpdf/index.php",
    "license": ["GPL-1.0+"],
    "authors": [
        {
            "name": "Ian N Back "
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=4.3.10",
        "ext-mbstring": "*"
    },
    "autoload": {
        "classmap": ["mpdf.php", "classes"]
    }
}
